char const* sourceCode = R"(
	contract c {
		function foo(uint a, uint b, uint c) returns (bytes32 d)
		{
			d = keccak256(a, b, c);
		}
	}
)";
compileAndRun(sourceCode);

ABI_CHECK(callContractFunction("foo(uint,uint,uint)", 10, 12, 13), encodeArgs(
	dev::keccak256(
		toBigEndian(u256(10)) +
		toBigEndian(u256(12)) +
		toBigEndian(u256(13))
	)
));