contract c {
		bytes data;
		function test() returns (uint) {
			for (uint i = 1; i < 40; i++)
			{
				data.push(byte(i));
				if ((data.length) != i) return 0x1000 + i;
				if (data[(data.length) - 1] != byte(i)) return i;
			}
			for (i = 1; i < 40; i++)
				if (data[i - 1] != byte(i)) return 0x1000000 + i;
			return 0;
		}
	}

contract main {

    function main() { 
          new c(); 
          functionCall(-1;0;test;;msginfo(-1,0,2,2));
    }

}
