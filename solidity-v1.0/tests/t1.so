
contract A {
	 function g(int a) public returns (int r) {
	    a = 2;
	    int c ;
	    c = functionCall(new B(); g1; a);
	    int b;
	    b = a + 2;
	    return b;
	 }
}

contract B {
	 function g1(int a) public returns (int r) {
	    return a + 4;
	 }
}


contract C {
	 function f (int x) public returns (int y)  {
	 	  int sum;
		  if (x < 0 ) {
		     sum = 0;
		  }
		  else {
		    sum = x + functionCall(f ; x - 1);
		  }
		  return sum;
		  
	}
}

